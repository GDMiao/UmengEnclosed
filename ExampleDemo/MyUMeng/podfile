# platform :ios, '8.0'

target 'MyUMeng' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for MyUMeng
  # 依赖库
    pod 'UMCCommon'
    pod 'UMCSecurityPlugins'
  # U-Share SDK UI模块（分享面板，建议添加）
    pod 'UMCShare/UI'
